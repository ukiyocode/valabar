<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.0"/>
  <object class="ValaBar" id="window">
    <property name="can-focus">False</property>
    <property name="title" translatable="yes">ValaBar</property>
    <property name="default-width">400</property>
    <property name="default-height">30</property>
    <property name="type-hint">dock</property>
    <property name="decorated">False</property>
    <property name="x">620</property>
    <property name="y">850</property>
    <signal name="destroy" handler="gtk_main_quit" swapped="no"/>
    <child>
      <object class="GtkGrid">
        <property name="visible">True</property>
        <property name="can-focus">False</property>
        <child>
          <object class="TaskBar" id="taskbar">
            <property name="orientation">GTK_ORIENTATION_HORIZONTAL</property>
            <property name="spacing">1</property>
            <child>
              <object class="AppButton">
                <property name="desktop-file">nemo.desktop</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
